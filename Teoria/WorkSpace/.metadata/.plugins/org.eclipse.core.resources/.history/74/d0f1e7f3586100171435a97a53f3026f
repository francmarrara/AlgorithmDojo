public class Nodo {
	private int key;
	private String name;

	private Nodo leftChild;
	private Nodo rightChild;

	public Nodo(int key, String name) {

		this.key = key;
		this.name = name;

	}

	public int getKey() {
		return key;
	}

	public void setKey(int key) {
		this.key = key;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String toString() {

		return name + " has the key " + key;

		/*
		 * return name + " has the key " + key + "\nLeft Child: " + leftChild +
		 * "\nRight Child: " + rightChild + "\n";
		 */

	}

	public Nodo getLeftChild() {
		return leftChild;
	}

	public void setLeftChild(Nodo leftChild) {
		this.leftChild = leftChild;
	}

	public Nodo getRightChild() {
		return rightChild;
	}

	public void setRightChild(Nodo rightChild) {
		this.rightChild = rightChild;
	}
}
